            android:id="@+id/p5"            android:textColor="#000"
            android:layout_below="@+id/p4"
            android:layout_alignParentStart="true" />

    </RelativeLayout>

                    <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                android:background="#EE7469"
                android:layout_weight="0.21" >


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Approximate Duration:"
                    android:id="@+id/aduration"
                    android:textColor="#383732"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Approximate Cost:"
                    android:id="@+id/textView34"
                    android:textColor="#383732"
                    android:layout_below="@+id/duration"
                    android:layout_alignStart="@+id/duration" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
